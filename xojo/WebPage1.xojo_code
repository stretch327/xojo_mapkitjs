#tag Events SOSMapKit1
	#tag Event
		Sub Ready()
		  
		  // California
		  Me.SetRegion(37.3316850890998, -122.030067374026, 2, 4)
		  Me.SetCameraDistance(2000)
		  Me.MinimumZoomRange = 500
		  Me.MaximumZoomRange = 2000
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Clicked(latitude as Double, longitude as Double)
		  label1.Text = latitude.ToString + " x " + longitude.ToString
		  
		  
		  dim annot as New SOSMapKitAnnotation(latitude, longitude, "X", &c000088, "marks the spot", False, "‚ùå")
		  SOSMapKit1.AddAnnotation(annot, True)
		End Sub
	#tag EndEvent
#tag EndEvents
